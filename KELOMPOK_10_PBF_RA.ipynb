        "2. Buatlah pemrograman berbasis objek pada soal no 1"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 1,
      "metadata": {},
      "outputs": [
        {
          "data": {
            "text/plain": [
              ""
            ]
          },
          "execution_count": 1,
          "metadata": {},
          "output_type": "execute_result"
        }
      ],
      "source": [
        "class Value:\n",
        "    def __init__(self, value):\n",
        "        self.value = value\n",
        "# `value >> func`\n",
        "    def __rshift__(self, other):\n",
        "        return other(self.value)\n",
        "def f(x):\n",
        "    return Value(4**x)\n",
        "def g(x):\n",
        "    return Value(x ** x)\n",
        "def h(x):\n",
        "    return Value(3 + x)\n",
        "result = Value(4) >> f >> g >> h\n",
        "result.value\n",
        "# result"
      ]
    }
